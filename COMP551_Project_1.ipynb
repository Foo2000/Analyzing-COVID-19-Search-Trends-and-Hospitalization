  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "nClusters = 6    # To be tuned"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Clustering on PCA-reduced data\n",
    "kmeansRed = KMeans(n_clusters=nClusters, random_state=0)\n",
    "kmeansRed.fit(reducedFeat)\n",
    "y_PredRed = kmeansRed.predict(reducedFeat)"
   ]
